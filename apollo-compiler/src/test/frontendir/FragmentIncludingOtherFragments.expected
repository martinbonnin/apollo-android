package com.example

import kotlin.Float
import kotlin.String

interface GetAnimal {
  interface Query : QueryFragment {
    val randomAnimal: com.example.Animal?

    interface AnimalCat : CatFragment, LionFragment {
      val name: String

      val habitat: com.example.Habitat

      interface Habitat {
        val temperature: Float?
      }
    }

    interface AnimalLion : CatFragment, LionFragment {
      val name: String
    }

    interface Animal : CatFragment, LionFragment
  }
}

interface QueryFragment {
  interface Query {
    val randomAnimal: com.example.Animal?

    interface AnimalCat : CatFragment, LionFragment {
      val name: String

      val habitat: com.example.Habitat

      interface Habitat {
        val temperature: Float?
      }
    }

    interface AnimalLion : CatFragment, LionFragment {
      val name: String
    }

    interface Animal : CatFragment, LionFragment
  }
}

interface CatFragment {
  interface Cat {
    val name: String

    val habitat: com.example.Habitat

    interface Habitat {
      val temperature: Float?
    }
  }
}

interface LionFragment {
  interface Lion {
    val name: String
  }
}
